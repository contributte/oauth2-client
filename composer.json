{
  "name": "contributte/oauth2-client",
  "description": "Integration of league/oauth2-client into Nette framework",
  "keywords": [
    "nette",
    "oauth2",
    "client"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/oauth2-client",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.2",
    "league/oauth2-client": "^2.6.0",
    "nette/http": "^3.0.5"
  },
  "require-dev": {
    "contributte/phpstan": "^0.2.0",
    "contributte/qa": "^0.4.0",
    "contributte/tester": "^0.3.0",
    "league/oauth2-facebook": "^2.0.5",
    "league/oauth2-google": "^3.0.3",
    "mockery/mockery": "^1.6.12",
    "nette/application": "^3.1.0",
    "nette/di": "^3.0.0",
    "omines/oauth2-gitlab": "^3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\OAuth2Client\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
